{
  "name": "wfuwp",
  "version": "0.1.2",
  "description": "CLI tool for WFU WordPress management tasks",
  "main": "dist/index.js",
  "bin": {
    "wfuwp": "./bin/wfuwp"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "release": "scripts/release.sh"
  },
  "keywords": [
    "cli",
    "wordpress",
    "aws",
    "s3",
    "wfu"
  ],
  "author": "Alexander Wright",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md"
  ]
}
